{
  "name": "@unisonjs/vue",
  "version": "0.1.4",
  "packageManager": "pnpm@8.15.3",
  "type": "module",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "scripts": {
    "build:esm": "tsc --noCheck --module ESNext --outDir ./dist/esm",
    "build:cjs": "tsc --noCheck --module CommonJS --moduleResolution null --outDir ./dist/cjs",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs && sh replace_constants.sh",
    "prepublishOnly": "npm run build",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "require": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "global.d.ts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "release-it-pnpm": {}
    }
  },
  "devDependencies": {
    "@unisonjs/core": "workspace:*",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/react": "^18.3.12",
    "api-extractor": "^99.99.99",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsc-alias": "^1.8.10",
    "unplugin-inline-enum": "^0.2.0"
  },
  "peerDependencies": {
    "@unisonjs/core": ">=0.1.1"
  },
  "dependencies": {
    "@vue/shared": "^3.5.13"
  }
}
